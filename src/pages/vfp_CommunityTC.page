        <div class="twelve columns">

          <!--   <h1>Teanancy Contracts</h1>  

             <hr />-->

             <table class="table table-striped table-bordered" width="100%">
                     

                    <tbody>
                        <tr class="success">
                            <th width="20%" style="text-align:center;line-height: 17px;">Tenancy Contract Number</th> 
                            <th width="30%" style="text-align:center;line-height: 17px;">Term Commencement Date</th> 
                            <th width="30%" style="text-align:center;line-height: 17px;">Contract Expiry Date</th>
                            <th width="20%" style="text-align:center;line-height: 17px;">Status</th>
                        </tr>
                    </tbody>
                    <apex:repeat value="{!TCList}" var="item"  > 
                        <tr>
                            <td style="text-align:center;line-height: 15px;"> <font style="cursor:pointer;text-decoration: underline;" onclick="window.open('/Portal/apex/vfp_CommunityTCDetails?TenancyContractId={!item.Id}', '_self');" target="_self"><apex:outputField value="{!item.Tenancy_Contract_Number__c}"/></font> </td> 
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.Term_Commencement_Date__c}" /> </td>
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.Contract_Expiry_Date__c}" /> </td>
