<apex:page Controller="cls_CommunityCaseLayout" 
           showChat="false" showHeader="false" docType="html-5.0" standardStylesheets="false" applyBodyTag="false" applyHtmlTag="false" > 
    <html class="no-js" lang="en">
        <head>
            <meta charset="utf-8" />
            <title>Waha</title>
            
            <link rel="stylesheet" media="all" href="{!URLFOR($Resource.WahaTemp02, 'WahaTemp02/css/base.css')}"/>
            <link rel="stylesheet" media="all" href="{!URLFOR($Resource.WahaTemp02, 'WahaTemp02/css/main.css')}"/>
            <style>
                table, tr, th, td{
                border-top: 1px solid #7d7e80 !important;
                }
            </style>
        </head>
        <body class="inner">
            
                <apex:form id="form">
        <apex:pageMessages escape="false" rendered="true"></apex:pageMessages>
        
        <apex:pageBlock id="EsRate" rendered="true">
                
                <header id="main-header">

    <div class="row header-inner">

          <div class="logo">
             <a class="smoothscroll" href="#hero">Waha</a>
          </div>

          <nav id="nav-wrap">         
             
             <a class="mobile-btn" href="#nav-wrap" title="Show navigation">
                <span class='menu-text'>Show Menu</span>
                <span class="menu-icon"></span>
             </a>
            <a class="mobile-btn" href="#" title="Hide navigation">
                <span class='menu-text'>Hide Menu</span>
                <span class="menu-icon"></span>
            </a>         

                                         <ul id="nav" class="nav">
                                
                                <li><a class="smoothscroll" href="{!$Site.BaseUrl}">Home.</a></li>
                                <!-- Logged in user -->
                                <apex:outputPanel rendered="{! If($Profile.Name =='Waha Customer Community Plus User' ,true,false) }">
                                   <li><a class="smoothscroll" href="{!$Site.BaseUrl}/apex/vfp_CommunityTC">Tenancy Contract.</a></li>
                                    <li  class="current"><a class="smoothscroll" href="{!$Site.BaseUrl}/apex/vfp_CommunityCases">Enquiries.</a></li>
                                    <li><a class="smoothscroll" href="{!$Site.Prefix}/secur/logout.jsp"> Logout.</a></li>
                                    <!--<li><apex:outputLink value="{!$Site.Prefix}/secur/logout.jsp" rendered="{!NOT(ISPICKVAL($User.UserType,'Guest'))}">{!$Label.site.logout}</apex:outputLink></li>-->
                                </apex:outputPanel>
                            </ul> 
                            

          </nav> <!-- /nav-wrap -->       

       </div> <!-- /header-inner -->

   </header>
     
     <div class="inside-page">
   <section id="portfolio">

      <div class="row section-head">

        <div class="twelve columns">
            <apex:outputPanel rendered="{! If($Profile.Name =='Waha Customer Community Plus User' ,true,false) }">
                <h4 Style="padding-bottom: 20px;"> Enquiry Information </h4>
                
                         <table class="table" width="100%" border="0">
                    <tbody >
                        <tr>
                            <td class="labelCol" style ="text-align: right ;padding-top: 0; padding-bottom: 0;" width="25%" ><label>Case Number</label></td>
                            <td class="dataCol col02" style ="text-align: left ;padding-top: 0; padding-bottom: 0;" width="25%"><apex:outputField value="{!selectedCase.CaseNumber}" /></td>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0;" width="25%"><label>Type</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0;" width="25%"><apex:outputField label="Type" value="{!selectedCase.Type}"/></td>
                        </tr>
                        <tr>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0;"><label>Tenants Name</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0;"><apex:outputField label="Tenants Name" value="{!selectedCase.Account.Name}"/></td>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0;"><label>Requested by</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0;"><apex:outputText label="Requested by" value="{!contactName}"/></td>
                            
                        </tr>
                        <tr>
                            <td class="labelCol" style ="text-align: right ;padding-top: 0; padding-bottom: 0"><label>Tenancy Contract</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField label="Tenancy Contract" value="{!selectedCase.Tenancy_Contract_Number__c}" /></td>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Status</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField label="Status" value="{!selectedCase.Status}"/></td>
                        
                        </tr>
                        <tr>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Unit</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField label="Unit" value="{!selectedCase.Unit__c}" />
                            </td>
                             <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Location</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField label="Location" value="{!selectedCase.Location__c}" />
                            </td>
                        </tr>
                        <tr>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>TIF Refrence No</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField value="{!selectedCase.TIF_Refrence_No__c}" />
                            </td>
                             <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Cost Responsibility</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField value="{!selectedCase.Cost_Responsibility__c}" />
                            </td>
                        </tr>
                        <tr>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Date/Time Opened</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField label="Date/Time Opened" value="{!selectedCase.CreatedDate}" /></td>
                            <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Date/Time Closed</label></td>
                            <td class="dataCol col02" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField label="Date/Time Closed" value="{!selectedCase.ClosedDate}" />
                            </td>
                        </tr>
                                                
                        <tr>
                             <td class="labelCol" style ="text-align: right;padding-top: 0; padding-bottom: 0"><label>Description of works/Tenants request</label></td>
                            <td class="dataCol col02" colspan="3" style ="text-align: left;padding-top: 0; padding-bottom: 0"><apex:outputField value="{!selectedCase.Description}" />
                            </td>
                         </tr>

                       
                    </tbody>
                </table>
                
                 <div align="center">
                    <apex:commandButton value="Back to Enquiry List" action="{!redirectCaseList}" />
                </div>
                </apex:outputPanel>
            </div>
                    </div>
                    </section>
                    </div>
                        
                        
                        <footer>
                    
                    <div class="row">       
                        
                        <div class="six columns tab-whole footer-about">
                            <p class="copyright">&copy; Copyright 2016 Waha. Developed by <a href="http://cloudconceptgroup.com/" target="_blank" >Cloud Concerpt.</a></p>  
                            
                        </div> <!-- /footer-about -->
                        
                        <div class="three columns tab-whole right-cols">
                            
                            <div class="row">
                                
                                <h3 class="contact">Follow Us</h3>
                                
                                
                            <ul class="footer-list">
                     <li><a href="https://www.linkedin.com/company/waha-capital?trk=top_nav_home" target="_blank">Linkedin</a></li>
                     <li><a href="https://twitter.com/WahaCapital"  target="_blank">Twitter</a></li>
                  </ul>          
                                
                                
                            </div> <!-- /Row(nested) -->
                            
                        </div>
                        
                        
                        
                        
                        
                        
                    </div> <!-- /row -->
                    
                </footer>
        </apex:pageBlock>
        </apex:form>
                </body>

    </html>  
</apex:page>