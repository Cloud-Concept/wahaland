<apex:page Controller="cls_CommunityCases"  showChat="false" showHeader="false" docType="html-5.0" standardStylesheets="false" applyBodyTag="false" applyHtmlTag="false" > 
    <html class="no-js" lang="en">
<head>
    <meta charset="utf-8" />
        <title>Waha</title>

    <link rel="stylesheet" media="all" href="{!URLFOR($Resource.WahaTemp02, 'WahaTemp02/css/base.css')}"/>
    <link rel="stylesheet" media="all" href="{!URLFOR($Resource.WahaTemp02, 'WahaTemp02/css/main.css')}"/>
</head>
 <apex:form id="myForm">
            
            <body class="inner">
   <header id="main-header">

    <div class="row header-inner">

          <div class="logo">
             <a class="smoothscroll" href="#hero">Waha</a>
          </div>

          <nav id="nav-wrap">         
             
             <a class="mobile-btn" href="#nav-wrap" title="Show navigation">
                <span class='menu-text'>Show Menu</span>
                <span class="menu-icon"></span>
             </a>
            <a class="mobile-btn" href="#" title="Hide navigation">
                <span class='menu-text'>Hide Menu</span>
                <span class="menu-icon"></span>
            </a>         

             <ul id="nav" class="nav">
                 
                 <li><a class="smoothscroll" href="{!$Site.BaseUrl}">Home.</a></li>
                 <!-- Logged in user -->
                 <apex:outputPanel rendered="{! If($Profile.Name =='Waha Customer Community Plus User' ,true,false) }">
                     <li><a class="smoothscroll" href="{!$Site.BaseUrl}/apex/vfp_CommunityTC">Tenancy Contract.</a></li>
                    <li class="current"><a class="smoothscroll" href="{!$Site.BaseUrl}/apex/vfp_CommunityCases">Enquiries.</a></li>
                     <li><a class="smoothscroll" href="{!$Site.Prefix}/secur/logout.jsp"> Logout.</a></li>
                     <!--<li><apex:outputLink value="{!$Site.Prefix}/secur/logout.jsp" rendered="{!NOT(ISPICKVAL($User.UserType,'Guest'))}">{!$Label.site.logout}</apex:outputLink></li>-->
                 </apex:outputPanel>
             </ul> 

          </nav> <!-- /nav-wrap -->       

       </div> <!-- /header-inner -->

   </header> 
     <div class="inside-page">
   <section id="portfolio">

      <div class="row section-head">

        <div class="twelve columns">

            <apex:pageBlock rendered="{! If($Profile.Name =='Waha Customer Community Plus User' ,true,false) }">
                    <apex:commandButton value="Submit New Enquiry" action="{!redirectNewCase}" />
                         <table class="table table-striped table-bordered" width="100%">

                    <tbody>
                        <tr class="success">
                            <th width="10%" style="text-align:center;line-height: 17px;">Case Number</th> 
                            <th width="20%" style="text-align:center;line-height: 17px;">Tenancy Contract</th> 
                            <th width="10%" style="text-align:center;line-height: 17px;">Unit</th>
                            <th width="10%" style="text-align:center;line-height: 17px;">Type</th>
                            <th width="10%" style="text-align:center;line-height: 17px;">Status</th>
                            <th width="20%" style="text-align:center;line-height: 17px;">Date/Time Opened</th>
                            <th width="20%" style="text-align:center;line-height: 17px;">Date/Time Closed</th>
                        </tr>
                    </tbody>
                    <apex:repeat value="{!AccountCasses}" var="item"  > 
                        <tr>
                            <td style="text-align:center;line-height: 15px;"> <font style="cursor:pointer;text-decoration: underline;" onclick="window.open('/Portal/apex/vfp_CommunityCaseLayout?CaseId={!item.Id}', '_self');" target="_self"><apex:outputText value="{!item.CaseNumber}"/></font> </td> 
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.Tenancy_Contract_Number__c}" /> </td>
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.Unit__c}" /> </td>
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.Type}" /> </td>
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.Status}" /> </td>
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.CreatedDate}" /> </td>
                            <td style="text-align:center;line-height: 15px;"> <apex:outputField value="{!item.ClosedDate}" /> </td>
                        </tr> 
                    </apex:repeat> 
                </table>
               <!--<apex:pageBlockButtons location="bottom">-->
                    <apex:commandButton action="{!Beginning}" title="Beginning" value="<<" disabled="{!disablePrevious}" reRender="myForm"/>
                    <apex:commandButton action="{!Previous}" title="Previous" value="<" disabled="{!disablePrevious}" reRender="myForm"/>        
                    <apex:commandButton action="{!Next}" title="Next" value=">" disabled="{!disableNext}" reRender="myForm"/>
                    <apex:commandButton action="{!End}" title="End" value=">>" disabled="{!disableNext}" reRender="myForm"/>   
               <!-- </apex:pageBlockButtons>-->
                <apex:outputLabel >Page {!pageNumber} of {!totalPages}</apex:outputLabel>
            </apex:pageBlock>    
     </div>

      </div>

        <!-- /row -->
      
   </section> <!-- /Portfolio -->



</div>
  <footer>

      <div class="row">       

         <div class="six columns tab-whole footer-about">
         <p class="copyright">&copy; Copyright 2016 Waha. Developed by <a href="http://cloudconceptgroup.com/" target="_blank" >Cloud Concept.</a></p>  

         </div> <!-- /footer-about -->

         <div class="three columns tab-whole right-cols">

            <div class="row">

                  <h3 class="contact">Follow Us</h3>
                  
            
                  <ul class="footer-list">
                     <li><a href="https://www.linkedin.com/company/waha-capital?trk=top_nav_home" target="_blank">Linkedin</a></li>
                     <li><a href="https://twitter.com/WahaCapital"  target="_blank">Twitter</a></li>
                  </ul>       

               
            </div> <!-- /Row(nested) -->

         </div>




       

      </div> <!-- /row -->

   </footer>
</body>

               
        </apex:form>
    </html>  
</apex:page>